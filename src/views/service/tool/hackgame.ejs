<div class="uk-width-1-1">
    <h3 class="uk-text-lead">Our Store</h3>
    <div class="uk-grid" data-uk-grid>
       <% if(tool && tool.length > 0){ %>
        <% tool.forEach(item => { %>
          
          <div class="uk-width-1-5@l uk-width-1-3@m uk-width-1-2@s">
            <%- include("../../../components/ui/ejs/toolgame.ejs",{url:item.slug,name:item.name,image:item.image,category:item.category_name,price:item.price}) %>
          </div>
        <% }) %>
        <% } %>
 
      </div>
      
</div>